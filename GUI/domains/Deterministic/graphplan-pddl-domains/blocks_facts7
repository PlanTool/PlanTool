(preconds
	(on-table blockA)
	(on blockB blockA)
	(on blockC blockB)
	(on blockD blockC)
	(on blockE blockD)
	(on blockF blockE)
	(on blockG blockF)
	(clear blockG)
	(arm-empty))

(effects
	(on blockB blockA)
	(on blockC blockB)
	(on blockD blockC)
	(on blockE blockD)
	(on blockF blockE)
	(on blockA blockG)
