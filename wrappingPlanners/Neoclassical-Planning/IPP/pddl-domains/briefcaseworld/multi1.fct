(define (problem multi1)
    (:domain multi-briefcase)
    (:objects l1 l2 l3 l4 home - location
              o1 o2 o3 o4  - portable
              b1 b2 b3 - briefcase)
    (:init  (is-at b1 home) (is-at b2 home) (is-at b3 home)
            (at o1 l1) (at o2 l2) (at o3 l3) (at o4 l4)) 

    (:goal (and (is-at b1 home) (is-at b2 home) (is-at b3 home)
      (at o1 home) (at o2 home) (at o3 home) (at o4 home))))
     

     

