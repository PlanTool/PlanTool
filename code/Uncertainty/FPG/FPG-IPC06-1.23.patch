diff -u -r -N mdpsim-1.23/benchmark-dir.sh FPG-IPC06/benchmark-dir.sh
--- mdpsim-1.23/benchmark-dir.sh	1970-01-01 01:00:00.000000000 +0100
+++ FPG-IPC06/benchmark-dir.sh	2007-01-12 10:44:28.000000000 +0100
@@ -0,0 +1,34 @@
+#! /bin/sh
+
+if [ $# != 2 ]
+then
+    echo "I need an input and an ouput directory"
+    exit
+fi
+
+if [ ! -d $1 ]
+then
+    echo "$1 not a directory"
+    exit
+fi
+
+export SEED=1
+mkdir $2
+
+for i in `ls $1`
+do
+    echo File: $i
+    for j in `\
+    cat $1/$i\
+    | grep problem\
+    | sed -e 's/(//g'\
+    | sed -e 's/)//g'\
+    | sed -e 's/\r//g'\
+    | sed -e 's/define//g'\
+    | sed -e 's/problem //g'\
+    | sed -e 's/ //g'`
+    do
+	echo File: $i Problem: $j Log-file: log-$j-${SEED}.log
+	./fpgclient 0:2323 $1/$i $j ${SEED} >& $2/log-$j-${SEED}.log
+    done
+done
