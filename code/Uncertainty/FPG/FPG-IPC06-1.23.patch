@@ -0,0 +1,15 @@
+#!/bin/sh
+
+while read file ; do
+
+# Split into one of two files for our two planning clients.
+        prob=`cat $file | grep problem | sed -e 's/.*(problem \(.*\))/\1/'`
+	if [ $ODD -eq 1 ] ; then
+	  echo $file $prob >> $1
+          ODD=0
+	else
+	  echo $file $prob >> $2
+          ODD=1
+	fi
+	  
+done < problem-files.txt
