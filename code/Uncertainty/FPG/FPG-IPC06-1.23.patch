+
+int connect(const char *hostname, int port)
+{
+  struct hostent *host = ::gethostbyname(hostname);
+  if (!host) {
+    perror("gethostbyname");
+    return -1;
+  }
+
+  int sock = ::socket(PF_INET, SOCK_STREAM, 0);
+  if (sock == -1) {
+    perror("socket");
+    return -1;
+  }
+  
+  struct sockaddr_in addr;
+  addr.sin_family=AF_INET;
+  addr.sin_port=htons(port);
+  addr.sin_addr = *((struct in_addr *)host->h_addr);
+  memset(&(addr.sin_zero), '\0', 8);
+
+  if (::connect(sock, (struct sockaddr*)&addr, sizeof(addr)) == -1) {
+    perror("connect");
+    return -1;
+  }
+  return sock;
+  //remember to call close(sock) when you're done
+}
+
+int main(int argc, char **argv)
+{
+  /* Set default verbosity. */
+  verbosity = 0;
+  /* Set default warning level. */
+  warning_level = 1;
+  if (argc != 7) {
+    std::cerr << "Usage: " << *argv << " <host>:<port> <problem file> <problem name> <seed> <ipc6|ipc4 mode> <timelimit>" << std::endl;
+    return 1;
+  }
+
+  ipc6 = (strcmp(argv[5],"ipc6") == 0);
+  if (ipc6)
+      cout<<"ipc6 mode"<<endl;
+  else
+      cout<<"ipc4 mode"<<endl;
+
+  int seed = atoi(argv[4]);
+  srandom(seed);
+  cout<<"Seed "<<seed<<endl;
+
+  if (!read_file(argv[2])) {
+    std::cerr << "Couldn't read problem file " << argv[2] << std::endl;
+    return 1;
+  }
+
+  const Problem *problem = Problem::find(argv[3]);
+  if (!problem) {
+    std::cerr << "Problem " << argv[3] << " is not defined in " << argv[2] << std::endl;
+    return 1;
+  }
+
+  int maxTime = atoi(argv[6]);
+  assert(maxTime > 0 && maxTime < 60*40);
+  cout<<"Optimisation time set to "<<maxTime;
+
+  
+  char *hostport = argv[1];
+  char *host = strtok(hostport, ":");
+  char *portstr = strtok(0, ":");
+  int port = atoi(portstr);
+
+  std::cerr << host <<" "<<port<<endl;
+  int socket = connect(host, port);
+  if (socket <= 0) {
+    std::cout << "Could not connect to " << argv[1] << std::endl;
+    return 1;
+  }
+#if defined __GNUC__ && __GNUC__ >= 3 && (__GNUC__>3 ||__GNUC_MINOR__ > 0)
+  //__gnu_cxx::stdio_filebuf<char> ofbuf(socket, std::ios_base::out, false, BUFSIZ);
+  //__gnu_cxx::stdio_filebuf<char> ifbuf(socket, std::ios_base::in, false, 1);
+  __gnu_cxx::stdio_filebuf<char> ofbuf(socket, std::ios_base::out, BUFSIZ);
+  __gnu_cxx::stdio_filebuf<char> ifbuf(socket, std::ios_base::in, 1);
+#else
+  std::filebuf ofbuf(socket);
+  std::filebuf ifbuf(socket);
+#endif
+  std::ostream os(&ofbuf);
+  std::istream is(&ifbuf);
+
+  FPGPlanner p(*problem);
+
+#ifndef HAVE_RATIONALS
+  std::cerr<<"Compiled with doubles instead of rationals."<<endl;
+#else
+  std::cerr<<"Compiled with rationals."<<endl;
+#endif
+
+
+  getLock(OPT_LOCK);
+  p.optimise(maxTime);
+  releaseLock(OPT_LOCK);
+
+  getLock(SERVE_LOCK);
+  XMLClient(&p, problem, "fpgclient", is, os);
+  sleep(3); // Just give the server a few secs to reset.
+  releaseLock(SERVE_LOCK);
+  
+  //close(socket);
+
+  return 0;
+}
