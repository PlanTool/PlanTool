+    exit(0);
+}
+
+
+FPGsim::FPGsim(FPGPlanner* p, int m) {
+ 
+    planner = p;
+
+    actionDim = 1;
+    obsRows = planner->nRows+1;
+    obsCols = 1;
+    actions = planner->nCols;
+    agents = planner->nCols;
+
+    s = NULL;
+    allGoalsComputed = cannotGenerateGoal = false;
+    distFromGoal = 0;
+    nSteps = 0;
+    max_plan_length = m;
+}
+
+
+void FPGsim::setInitState()
+{
+    if (distFromGoal <= 0) {
+	if (s != NULL)
+	    delete s;
+	s = new State(pb);
+	if (ipc6) {
+	    oldmetric = (pb).goal().progress(s->atoms(), s->values());
+	} else {
+#ifndef HAVE_RATIONALS
+	    oldmetric = (pb).metric().value(s->values()) ;
+#else
+	    oldmetric = (pb).metric().value(s->values()).double_value() ;
+#endif
+	}
+    }
+    else 
+	setRandomGoalState(distFromGoal);
+
+}
+
+
+void FPGsim::setRandomGoalState(int dist)
+{
+    SubstitutionMap subst;
+    
+    if (!allGoalsComputed) {
+	AllGoals = (StateFormula*) &(pb).goal().instantiation(subst, pb);
+	allGoalsComputed = true;
+	/*AllGoals.print(cerr,
+	  (pb).domain().predicates(),
+	  (pb).domain().functions(),
+	  (pb).terms()
+	  );
+	  std::cerr<<std::endl<<std::endl;*/
+    }
+
+    int attempts = 0;
+    bool deadend = false;
+    while (true) {
+	if (s != NULL)
+	    delete s;
+	//s = new State(pb);
+	
+	s = new State();
+	s->problem_ = &pb;
+	AllGoals->randomState(s, pb);
+	
+	/*std::cerr<<"s= ";
+	s->print(std::cerr,
+		 (pb).domain().predicates(),
+		 (pb).domain().functions(),
+		 (pb).terms());
+	std::cerr<<std::endl;*/
+
+	attempts++;
+
+	if ((pb).goal().holds(s->atoms(), s->values())) {
+	    State* next_s;
+	    const Action* a;
+	    int nEnabled;
+	    for(int i=0; i<dist; i++) {
+		enabledActions.clear();
+		(pb).enabled_actions(enabledActions, s->atoms(), s->values());
+		nEnabled = enabledActions.size();
+		if (nEnabled != 0) {
+		    a = planner->allActions[rand()%nEnabled];
+		    next_s = (State*) &s->next(a);
+		    delete s;
+		    s = next_s;
+		} else {
+		    //std::cerr<<"Generated state was a dead-end..."<<std::endl;
+		    deadend = true;
+		}
+	    }
+	    if (! deadend)
+		break;
+	}
+	if (attempts>100) {
+	    delete s;
+	    s = new State(pb);
+	    std::cerr<<"Unable to randomly generate a goal state..."<<std::endl;
+	    cannotGenerateGoal = true;
+	    distFromGoal = 0;
+	    break;
+	}
+    }
+
+    /*std::cerr<<"s= ";
+    s->print(std::cerr,
+	     (pb).domain().predicates(),
+	     (pb).domain().functions(),
+	     (pb).terms());
+    std::cerr<<std::endl;*/
+    
+    //exit(0);
+
+    if (ipc6) {
+	oldmetric = (pb).goal().progress(s->atoms(), s->values());
+    } else {
+#ifndef HAVE_RATIONALS
+	oldmetric = (pb).metric().value(s->values()) ;
+#else
+	oldmetric = (pb).metric().value(s->values()).double_value() ;
+#endif
+    }
+
+}
+
+// Return size of reward vector
+// OUTPUT: scalar reward
+int FPGsim::getRewardDim() {
+
+    return 1;
+
+}
+
+// Return reward vector
+// OUTPUT: scalar reward
+void FPGsim::getReward(Vector &rv) {
+
+    //cerr<<"r="<<reward<<endl;
+    rv(0) = reward;
+
+}
+
+
+// INPUT: matrix structure, 1x1, to return int observation in
+void FPGsim::getObservation(Observation& obs) {
+
+    // 1- get enabled/eligible actions
+    enabledActions.clear();
+    (pb).enabled_actions(enabledActions, s->atoms(), s->values());
+
+    for (Vector::iterator i = obs.getEligible().begin(); i != obs.getEligible().end(); i++)
+	*i = false;
+
+    //cerr<<"FPGsim::getObservation()"<<endl;
+    //cerr<<"Enabled actions:"<<endl;
+    for(ActionList::const_iterator ai = enabledActions.begin();
+	ai != enabledActions.end(); ai++)
+	{
+	    /*(*ai)->print(std::cerr,
+			 planner->_problem.terms()
+			 );
+	    cerr<<endl;*/
+	    obs.getEligible()(((Action*)*ai)->fpgId) = true;
+	}
+
+    //cerr<<"obs.eligible="<<obs.eligible<<endl;
+
+    // 2- get observations
+    obs.getFeatures().assign( scalar_matrix<double>(planner->nRows+1, 1, 0.));
+    for (AtomSet::const_iterator ai = s->atoms().begin();
+	 ai != s->atoms().end(); ai++)
+	obs.getFeatures()(((Atom*)*ai)->fpgId,0) = 1.;
+    obs.getFeatures()(planner->nRows+1-1, 0) = 1.;
+
+}
+
+
+// Do the action, core of the simulator
+// INPUT: action vector which will always be 1x1 in this case.
+int FPGsim::doAction(Vector& action) {
+
+    /*cerr<<"state= ";
+    s->print(std::cerr,
+	     planner->_problem.domain().predicates(),
+	     planner->_problem.domain().functions(),
+	     planner->_problem.terms()
+	     );
+    cerr<<endl;*/
+
+    const Action* a = planner->allActions[(int)action[0]];
+    //const Action* a = enabledActions[(int)action[0]];
+    /*cerr<<"action[0]="<<action[0]<<endl;
+    a->print(std::cerr, planner->_problem.terms() );
+    cerr<<endl;*/
+    const State& next_s = s->next(a);
+    /*cerr<<"next state= ";
+    next_s.print(std::cerr,
+	     planner->_problem.domain().predicates(),
+	     planner->_problem.domain().functions(),
+	     planner->_problem.terms()
+	     );
+	     cerr<<endl;*/
+    delete s;
+    s = (State*) &next_s;
+
+    if (ipc6) {
+	double metric = (pb).goal().progress(s->atoms(), s->values());
+	reward = 100 * ( metric - oldmetric );
+	oldmetric = metric;
+	//cerr<<"Progress="<<metric<<endl;
+    } else {
+	reward = 0.;
+/*
+#ifndef HAVE_RATIONALS
+	double metric = (pb).metric().value(s->values()) ;
+#else
+	double metric = (pb).metric().value(s->values()).double_value() ;
+#endif
+	reward = metric - oldmetric;
+	oldmetric = metric;
+*/
+    }
+
+    goal = (pb).goal().holds(s->atoms(), s->values());
+
+    if (goal)
+	{
+	    //cerr<<".";
+	    //cerr<<"goal reached "<<endl;
+	    reward += 1000;
+	    setInitState();
+	    goal = false;
+	    return 1;
+	}
+    else
+	{
+	    nSteps++;
+	    int m = (distFromGoal<=0) ? max_plan_length : distFromGoal*5;
+	    if ( nSteps > m )
+		{
+		    nSteps = 0;
+		    setInitState();
+		}
+	    return 0;
+	}
+}
