+static bool read_file(const char* name) {
+  yyin = fopen(name, "r");
+  if (yyin == NULL) {
+    std::cerr << PACKAGE << ':' << name << ": " << strerror(errno)
+	      << std::endl;
+    return false;
+  } else {
+    current_file = name;
+    bool success = (yyparse() == 0);
+    fclose(yyin);
+    return success;
+  }
