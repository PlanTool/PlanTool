--- mdpsim-1.23/FPGPomdp.cc	1970-01-01 01:00:00.000000000 +0100
+++ FPG-IPC06/FPGPomdp.cc	2007-01-12 10:44:27.000000000 +0100
@@ -0,0 +1,252 @@
+// $Id$
+
+#include<PGBasics.hh>
+#include<time.h>
+#include<iomanip>
+#include<ManyToOneController.hh>
+#include<ManyToOneControllerMinUpdates.hh>
+#include<OLPomdp.hh>
+
+using namespace std;
+using namespace libpg;
+
+#include"client.h"
+#include"fpg.h"
+#include"fpgsim.h"
+#include"FPGPomdp.h"
+
+#define REVERT_TO_PREVIOUS_THRESH 0.9
+#define CHANGE_THRESH 0.005// Values considered the same if within 0.5 %
+#define MAX_EPOCHS_WITHOUT_CHANGE 5
+
+// Top level piece of glue for Policy gradient, link the controller
+// and the simulator 
+// INPUT: Controller 
+// INPUT: Simulator (problem specific) 
+// INPUT: discount factor [0, 1]. If 1, trace must be reset
+// occasionally or have 0 mean reward.
+FPGPomdp::FPGPomdp(Controller* controller, FPGsim* simulator, double discount, double stepSize) : OLPomdp(controller, (Simulator*) simulator, discount, stepSize) {
+    episodeSteps = 0;
+    fpgsimulator = simulator;
+}
+
+
+
+// Do a fixed number of individual steps, aggregating the reward.  No
+// output is done. Could also be termed an "epoch" 
+// INPUT: empty vector to accumulate average reward for each reward dimension
+// INPUT: number ofsteps to iterate for 
+// INPUT: should learning be turned on, or are we
+// just exploiting/evaluating the current policy?
+double FPGPomdp::doSteps(Vector& totalRewards, int steps, bool learn) {
+
+    Observation obs(fpgsimulator->getObsRows(),fpgsimulator->getObsCols(),fpgsimulator->getAgents());
+    Vector action(fpgsimulator->getActionDim());
+    Vector rewards(fpgsimulator->getRewardDim());
+    rewards.clear();
+    Vector reinforcement(fpgsimulator->getRewardDim());
+    bool goalReached;
+    
+    for (int s = 0; s < steps; s++) {
+
+	fpgsimulator->getObservation(obs);
+	if (learn) {
+	    // Next line needed to make sure
+	    // ManyToOneControllerMinUpdates' "eligible" pointer is
+	    // initialised properly.
+	    if (s == 0) controller->getAction(obs, action, false);
+	    // Must do next line before calling getAction() since
+	    // getAction will accumulate log action gradients into the
+	    // trace directly for efficiency.
+	    controller->discountTrace();
+	}
+	controller->getAction(obs, action, learn);
+	episodeSteps++;
+	goalReached = (bool)fpgsimulator->doAction(action);
+	fpgsimulator->getReward(rewards);
+	totalRewards += rewards;
+	if (learn) {
+	    reinforcement.assign(rewards-baseline);
+	    // [daa] Check for 0 rewards is done at Controller level
+	    controller->instantStep(reinforcement);
+	}
+	if (goalReached) {	    
+	    controller->resetTrace();
+	    // [daa] why is this commented out? 
+	    //s += fpgsimulator->getMaxEpisodeLength() - episodeSteps;
+	    episodeSteps = 0;
+	    rewards.clear();
+	    goalReached=false;
+	}
+    }
+
+    totalRewards/=steps;
+    return norm_1(totalRewards)/fpgsimulator->getRewardDim();
+}
+
+
+// This version of learn() (see RLAlg::learn()) focuses on time
+// control (for the IPC).  It still "cuts" simulations in epochs
+// lasting a given number of time steps, but using a rather small
+// "stepsPerEpoch" for that.  A higher level loop is checking whether
+// the step-size should be adapted every "secsPerTEpoch" seconds
+// (typically 30 seconds ?).  So, this new function just takes an
+// additional parameter: "secsPerTEpoch".
+//
+// Additionally, there is a first learning phase where runs start from a state close to the goal.
+double FPGPomdp::learn(int secsPerTEpoch, int stepsPerEpoch,
+		    int maxTime, int maxSteps, double maxVal) {
+
+    int totalSteps = 0;
+    int secs = 0;
+    int epoch = 0;
+    int lastChange = 0;
+    double lastEpochVal = 0;
+    double prevEpochVal = 0;
+    double lastStepCheckVal = 0;
+    double bestEpochVal = 0;
+    Vector totalRewards(fpgsimulator->getRewardDim());
+    time_t  deadline;
+    
+
+    totalRewards.clear();
+
+    time_t startTime=time(NULL);
+    deadline = startTime;
+
+    cout<<setprecision(4);
+
+    fpgsimulator->setDistFromGoal(0);
+
+    /*
+    lastEpochVal += doSteps(totalRewards, stepsPerEpoch, false);
+    totalSteps += stepsPerEpoch;
+    lastStepCheckVal = bestEpochVal = lastEpochVal;
+    */
+
+    if (doSaves) write(saveName);
+
+    // Record time so far.
+    secs = time(NULL) - startTime;
+    printPerformanceInfo(true, totalSteps, lastEpochVal, controller->getMaxParam(), secs);
+
+    //if (saveName != NULL) write(saveName);
+    // Just do this once at the beginning.
+    controller->resetTrace();
+    
+    while (true) {
+	
+	// Halting conditions
+	if (maxTime && secs >= maxTime) break;
+	if (maxSteps && totalSteps >= maxSteps) break;
+	if (maxVal && lastEpochVal >= maxVal) break;
+	if (lastChange > MAX_EPOCHS_WITHOUT_CHANGE) {
+			cout<<"Value converged. Stopping.\n";
+			break;
+	}
+
+	// Computing next deadline.
+	time_t t=time(NULL);
+	do {
+	    deadline += secsPerTEpoch;
+	} while (t>deadline);
+
+	prevEpochVal = lastEpochVal;
+
+	// Do a sufficient number of learning iterations (each over stepsPerEpoch)
+	// to reach next deadline.
+	lastEpochVal = learnCore(stepsPerEpoch, totalSteps, deadline, epoch);
+	
+	// Record time so far.
+	secs = time(NULL) - startTime;
+	
+	// Save params if better than before
+	if (lastEpochVal > bestEpochVal) {
+	    bestEpochVal = lastEpochVal;
+	    if (doSaves) write(saveName);
+	}
+
+	// Detect if value has changed significantly
+	if (fabs(lastEpochVal - prevEpochVal)/lastEpochVal > CHANGE_THRESH) {
+		lastChange = 0;
+	} 
+	else lastChange++; 
+	
+	printPerformanceInfo(false, 
+			     totalSteps, 
+			     lastEpochVal, 
+			     controller->getMaxParam(), 
+			     secs);
+
+	if (epochsBetweenStepUpdates > 0 && epoch%epochsBetweenStepUpdates==0) {
+	    checkStepSize(lastEpochVal, bestEpochVal, lastStepCheckVal);
+	    lastStepCheckVal = lastEpochVal;
+	}
+
+	epoch++;
+    }
+    
+    // If we've gone badly downhil, restore the parameters.
+    if (lastEpochVal < bestEpochVal*REVERT_TO_PREVIOUS_THRESH) {
+	    	cout<<"Reverting to previous best parameters for final\n";
+	    	read(saveName);
+    }
+    return lastEpochVal;
+    
+}
+
+
+// This is a version of learnCore that does several epochs of size "stepsPerEpoch" as long
+// as a deadline (time in seconds) is not reached.
+double FPGPomdp::learnCore(int stepsPerEpoch, int& totalSteps, int deadline, int& epoch) {
+
+    double lastEpochVal=0;
+    Vector totalRewards(fpgsimulator->getRewardDim());
+    totalRewards.clear();
+
+    int i=0;
+    // Learn
+    while (true) {
+	
+	lastEpochVal += doSteps(totalRewards, stepsPerEpoch, true);
+	totalSteps += stepsPerEpoch;
+	i++;
+	
+	if (deadline <= time(NULL)) break;
+    }
+	
+    if (useAutoBaseline) {
+	baseline.assign(totalRewards);
+    }
+
+    return (double)lastEpochVal/(double)i;
+}
+
+
+
+// Do automatic scalar step size update. Will be redundant if using
+// SMD.  Very dumb: attempts to back off quickly if policy dies
+// catastrophically, and slowly increase if we stop learning fast
+// enough. Should help to go to a more deterministic policy.
+// This is not a substitute for a good initial step size, but it
+// will hopefully save you if things go bad half way through optimisation.
+// INPUT: current average reward
+// INPUT: best average reward
+// INPUT: average reward **last time checkStepSize** was called.
+void FPGPomdp::checkStepSize(double curr, double best, double last) {
+
+    // We should only be doing this if user requested auto
+    // step size updates
+    assert(epochsBetweenStepUpdates > 0); 
+
+    if (curr < last*(1.0 + threshold)) {
+	// We're learning too slowly. Speed up.
+	stepSize*=increaseFactor;
+	controller->setStepSize(stepSize);
+	cout<<"Policy improving too slowly. Step size increased to "<<stepSize<<endl;
+	return;
+    }
+	
+
+}
+
