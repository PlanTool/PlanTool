+void getLock(char* file) {
+
+    int fd = -1;
+    bool first=true;
+    
+    while (fd == -1) {
+    
+	fd = open(file, O_CREAT | O_EXCL, S_IRWXU); 
+	if (fd == -1) {
+	    if (errno == EEXIST) {
+	        if (first) {
+			std::cout<<"Blocking on '"<<file<<"'\n";
+			first=false;
+		}
+		sleep(LOCK_SLEEP_TIME);
+	    }
+	    else 
+		perror("Non locking file error: ");
+	    }
+    }
+
+    char pid[20];
+    sprintf(pid, "%d\n", getpid());
+    write(fd, pid, sizeof(char)*strlen(pid)+1);
+
+    close(fd);
+
+}
+
+void releaseLock(char* file) {
+
+    unlink(file);
+
