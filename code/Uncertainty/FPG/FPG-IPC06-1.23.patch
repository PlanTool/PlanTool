+void FPGPlanner::optimise(int maxTime)
+{
+    time_t startTime = time(NULL);
+
+    for (int i=0; i < TEST_RUNS; i++) {
+	//sim->setDistFromGoal(5);
+	sim->setInitState();
+	learner->learn(SECS_PER_TEPOCH, STEPS_PER_EPOCH, maxTime, MAX_STEPS);
+    }
+
+    cout<<"Took "<<time(NULL) - startTime<<" secs\n";
+
+    learner->restoreBestParameters();
+  
+}
+
+const Action* FPGPlanner::decideAction(const State& state)
+{
+    nSteps++;
+    if (nSteps > MAX_PLAN_LENGTH) {
+	cout<<"MAX_PLAN_LENGTH reached"<<endl;
+	return NULL;
+    }
+    
+    ActionList actions;
+    _problem.enabled_actions(actions, state.atoms(), state.values());
+    
+    if (actions.empty()) {
+	return NULL;
+    } else {
+	//size_t i = size_t(rand()/(RAND_MAX + 1.0)*actions.size());
+	//return actions[i];
+	sim->s = (State*) &state;
+	sim->getObservation(*serv_obs);
+	controller->getAction(*serv_obs, *serv_action, false);
+	return allActions[(int)(*serv_action)[0]];
+    }
+}
