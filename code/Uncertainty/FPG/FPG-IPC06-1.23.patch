+++ FPG-IPC06/makeComplete.sh	2007-01-12 10:44:37.000000000 +0100
@@ -0,0 +1,18 @@
+#!/bin/sh
+
+for file in * ; do
+	if [ -d $file ] ; then
+		cd $file;
+		pwd
+		if [ -e domain.pddl ] ; then
+			for problem in p*.pddl ; do
+				cat domain.pddl $problem > complete-$problem
+			done
+		else
+			for problem in p*.pddl ; do
+				cp $problem complete-$problem
+			done
+		fi	
+		cd ..
+	fi
+done
