+#define DISCOUNT 0.85
+//98
+//#define STEP_SIZE 0.000001
+//#define STEP_SIZE 0.0000001
+//#define STEPS_PER_EPOCH 10
+//#define MAX_STEPS 100
+#define SECS_PER_TEPOCH 20
+#define STEPS_PER_EPOCH 5000
+//#define STEPS_PER_EPOCH 100000
+#define MAX_STEPS 0
+//#define MAX_STEPS 10000000
+//#define MAX_TIME ((40*60)-45)
+#define TEST_RUNS 1
+#define NN_LAYERS 2 // includes input dimension and output dimension
+//#define NN_LAYERS 3 // includes input dimension and output dimension
+#define THRESH 0.05
+#define STEP_INC 2.0
+#define STEP_DEC 0.0
+#define EPOCHS_BETWEEN_STEP_CHECK 4
+#define MAX_PLAN_LENGTH MAX_TURNS
+#define MAX_RAND 0.00
+
+FPGPlanner::FPGPlanner(const Problem& problem) : Planner(problem)
+{
+
+    cout<<"[FPGPlanner::FPGPlanner()]"<<endl;
+
+    //    srand(time(0));
+
+    nCols = 0; // nCols will store the number of reachable actions.
+
+    // Start with a list of all instantiated actions.
+    ActionList remainingActions = _problem.actions();
+    ActionList actions;
+
+    //cout<<"Initial atoms:"<<endl;
+    // First make a list of atoms that can appear in the initial
+    // state.
+    for (AtomSet::const_iterator ai = _problem.init_atoms().begin();
+	 ai != _problem.init_atoms().end(); ai++) {
+	/*(*ai)->print(cout,
+		     _problem.domain().predicates(),
+		     _problem.domain().functions(),
+		     _problem.terms()
+		     );
+		     cout<<" ";*/
+	allAtomsSet.insert(*ai);
+	allAtomsList.push_back(*ai);
+    }
+    //cout<<endl;
+    for (EffectList::const_iterator ei = _problem.init_effects().begin();
+	 ei != _problem.init_effects().end(); ei++) {
+	/*(*ei)->print(cout,
+		     _problem.domain().predicates(),
+		     _problem.domain().functions(),
+		     _problem.terms()
+		     );
+		     cout<<" ";*/
+	(*ei)->listAtoms(allAtomsSet, allAtomsList);
+    }
+    //cout<<endl;
+
+    //cout<<"Going through enabled actions' effects:"<<endl;
+    // Then expand this list by looking at atoms which can be produced
+    // by enabled actions (remove actions which have already been
+    // used).
+    while (true) {
+
+	// Get enabled actions in the list of unused actions.
+	_problem.enabled_actions_noValues(remainingActions,
+					 actions,
+					 allAtomsSet);
+
+	// If no enabled actions: step out.
+	if (actions.size() == 0)
+	    break;
+
+	// For each enabled action:
+	for (ActionList::const_iterator ai = actions.begin();
+	     ai != actions.end(); ai++) {
+	    
+	    /*
+	    std::cout<<"action:\t";
+	    ((Action*)(*ai))->print(std::cout,_problem.terms());
+	    std::cout<<std::endl;//<<"effects:";
+	    */
+
+	    allActions.push_back(*ai);
+	    ((Action*)(*ai))->fpgId = nCols++ ; // Action is reachable -> give a valid id.
+
+	    // Add atoms from this action's effects.
+	    /*(*ai)->effect().print(cout,
+				  _problem.domain().predicates(),
+				  _problem.domain().functions(),
+				  _problem.terms()
+				  );
+				  cout<<endl;*/
+	    (*ai)->effect().listAtoms(allAtomsSet, allAtomsList);
+	    
+	    // Remove action from unused actions.
+	    ActionList::iterator af = remainingActions.begin();
+	    while((*af) != (*ai))
+		af++;
+	    remainingActions.erase(af);
+	    
+	}
+	actions.erase(actions.begin(),actions.end());
+
+    }
+
+    Atom* at = (Atom*) *allAtomsList.begin();
+    int size=0;
+    // Give Id numbers to reachable atoms:
+    for(AtomTable::const_iterator ai=at->atoms.begin();
+	ai!=at->atoms.end(); ai++)
+	{
+	    ((Atom*)*ai)->fpgId = -1;
+	    size++;
+	}
+    cout<<"atoms.size="<<size<<endl;
+
+    nRows = 0;
+    for(AtomList::const_iterator ai=allAtomsList.begin();
+	ai!=allAtomsList.end(); ai++)
+	{
+	    ((Atom*)*ai)->fpgId = nRows++;
+	    /*((Atom*)*ai)->print(cout,
+				_problem.domain().predicates(),
+				_problem.domain().functions(),
+				_problem.terms()
+				);
+				cout<<" ";*/
+	}
+    cout<<"allAtoms.size="<<nRows<<endl;
+
+    // Create neural networks
+#if (NN_LAYERS==2)
+    /* 2 layers */
+    cout<<"2 layers NN\n";
+    Vector nnDims(NN_LAYERS);
+    nnDims[0] = nRows+1;
+    nnDims[1] = 1;
+    Vector squash(NN_LAYERS);
+    squash[1] = 0;
+#else
+    // Create neural networks
+    /* 3 layers */
+    cout<<"3 layers NN\n";
+    Vector nnDims(NN_LAYERS);
+    nnDims[0] = nRows+1;
+    nnDims[1] = 4;
+    nnDims[2] = 1;
+    Vector squash(NN_LAYERS);
+    squash[1] = 1;
+    squash[2] = 0;
+#endif
+ 
+    	cout<<"step size="<<STEP_SIZE<<endl;
+    	cout<<"discount="<<DISCOUNT<<endl;
+		
+    Approximator* approx;
+    for(int i=0; i<nCols; i++) {
+	approx = new NeuralNet(nnDims, squash);
+	approximators.push_back(approx);
+    }
+
+    // Put neural networks into a brand new many-to-one controller
+    //controller = new MinUpdatesController(new ManyToOneController(approximators, false, false));
+    //controller = new ManyToOneController(approximators, false, false);
+    controller = new ManyToOneControllerMinUpdates(approximators, false, false);
+
+    // Create a simulator
+    sim = new FPGsim(this, MAX_PLAN_LENGTH);
+
+    // Put all that into an OLPomdp learner
+    learner = new FPGPomdp(controller,
+			  sim,
+			  DISCOUNT, 
+			  STEP_SIZE
+			  );
+    learner->setUseAutoBaseline(false);
+    learner->setEpochsBetweenStepUpdates(EPOCHS_BETWEEN_STEP_CHECK);
+    learner->setThreshold(THRESH);
+    learner->setIncreaseFactor(STEP_INC);
+    learner->setDecreaseFactor(STEP_DEC);
+
+    if (NN_LAYERS > 2)
+	controller->randomizeParams(MAX_RAND);
+
+    string suffix(_problem.name());
+    suffix += ".params";
+    learner->saveBest(strdup(suffix.c_str()));
+
+    // The following variables are used when interacting with the server:
+    serv_obs = new Observation(sim->obsRows,sim->obsCols,sim->agents);
+    serv_action = new Vector(sim->actionDim);
+
+}
